{
    "name": "shibuyakosuke/laravel-crud-command",
    "description": "Generate CRUD files for laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ShibuyaKosuke",
            "email": "shibuya@imagenious.co.jp"
        }
    ],
    "require": {
        "php": "^7.2.5 | ^7.3 | ^7.4",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "laravel/framework": "^6.0 | ^7.0 | ^8.0",
        "laravelcollective/html": "^6.1 | ^6.2",
        "shibuyakosuke/laravel-form-extend": "^1.0",
        "diplodocker/comments-loader": "^0.0.2",
        "shibuyakosuke/laravel-database-validation": "^1.2.0",
        "shibuyakosuke/laravel-model-replacement": "^1.1.1",
        "shibuyakosuke/laravel-crud-breadcrumbs": "^1.0.0",
        "maatwebsite/excel": "^3.1",
        "watson/rememberable": "^3.0 | ^4.0 | ^5.0"
    },
    "autoload": {
        "psr-4": {
            "ShibuyaKosuke\\LaravelCrudCommand\\": "src/",
            "ShibuyaKosuke\\LaravelCrudCommand\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShibuyaKosuke\\LaravelCrudCommand\\Providers\\CommandServiceProvider"
            ],
            "aliases": {
                "Stubs": "ShibuyaKosuke\\LaravelCrudCommand\\Facades\\Stub"
            }
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "mockery/mockery": "^1.3 | ^1.4",
        "orchestra/testbench": "^5.0 | ^6.0"
    },
    "minimum-stability": "dev"
}
